{
    "name": "creatr",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "next build",
        "check": "next lint && tsc --noEmit",
        "dev": "next dev",
        "lint": "eslint",
        "lint:fix": "eslint . --fix",
        "preview": "next build && next start",
        "start": "next start",
        "typecheck": "tsc --noEmit",
        "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
        "test": "jest",
        "test:types": "tsc --noEmit --pretty",
        "test:all": "pnpm run test && pnpm run test:types",
        "db:generate": "npx drizzle-kit generate && npx drizzle-kit migrate && git add . && git commit -m 'chore: update migration'",
        "db:setup": "bun run src/scripts/setup-database.ts",
        "db:init": "bun run src/db/init-mysql.js",
        "db:reset": "bun run src/scripts/reset-database.ts"
    },
